{
  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "declaration": true,
    "lib": ["es6", "esnext.asynciterable"],
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "preserveWatchOutput": true,
    "pretty": true,
    "strictNullChecks": true,
    "outDir": "build",
    "rootDir": "src"
  }
}
